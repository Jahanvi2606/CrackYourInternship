/**
 * @param {number[]} prices
 * @return {number}
 */
var maxProfit = function(prices) {
    let cur_prof= prices[0];
   let max_prof =0
let n=prices.length
    for(let i=1;i<prices.length;i++){
     if( cur_prof>prices[i]) {
        cur_prof = prices[i];
     }  
     max_prof = Math.max(max_prof,prices[i]-cur_prof);
    }
    return max_prof;
};
